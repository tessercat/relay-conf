<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<include>
  <configuration name="verto.conf" description="HTML5 Verto Endpoint">
    <settings>
      <param name="debug" value="10"/>
      <param name="enable-fs-events" value="false"/>
      <param name="enable-presence" value="false"/>
    </settings>
    <profiles>
      <X-PRE-PROCESS cmd="include" data="verto.conf.d/inbound-v4.xml"/>
      <X-PRE-PROCESS cmd="include" data="verto.conf.d/inbound-v6.xml"/>
    </profiles>
  </configuration>
</include>
